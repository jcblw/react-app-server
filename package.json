{
  "name": "react-component-server-example",
  "version": "1.0.0",
  "description": "A server that will render out isomophic react apps",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node -r dotenv/config lib/index.js",
    "dev-start": "nodemon -r dotenv/config lib/index.js",
    "compile": "babel src --stage 0 --modules common --out-dir lib",
    "watch": "babel src --stage 0 --modules common --out-dir lib --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jcblw/react-app-server.git"
  },
  "keywords": [
    "react",
    "server",
    "isomophic"
  ],
  "author": "Jacob Lowe",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jcblw/react-app-server/issues"
  },
  "homepage": "https://github.com/jcblw/react-app-server#readme",
  "dependencies": {
    "dotenv": "^4.0.0",
    "express": "^4.13.3",
    "invariant": "^2.2.0",
    "react": "^15.5.4",
    "react-component-server": "^1.0.0",
    "react-dom": "^15.5.4"
  },
  "devDependencies": {
    "babel": "^6.23.0",
    "nodemon": "^1.8.1"
  }
}
