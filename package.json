{
  "name": "react-app-server",
  "version": "1.0.0",
  "description": "A server that will render out isomophic react apps",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node -r dotenv/config index.js",
    "compile": "babel src --out-dir lib",
    "watch": "babel src --out-dir lib --watch",
    "postinstall": "npm run compile"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jcblw/react-app-server.git"
  },
  "keywords": [
    "react",
    "server",
    "isomophic"
  ],
  "author": "Jacob Lowe",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jcblw/react-app-server/issues"
  },
  "homepage": "https://github.com/jcblw/react-app-server#readme",
  "dependencies": {
    "browserify": "^12.0.1",
    "dotenv": "^1.2.0",
    "express": "^4.13.3",
    "react": "^0.14.3",
    "react-dom": "^0.14.3",
    "react-dom-stream": "^0.3.0"
  },
  "devDependencies": {
    "babel": "^6.1.18",
    "babel-cli": "^6.2.0",
    "babel-core": "^6.2.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.2.0",
    "babel-preset-es2015": "^6.1.18"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ],
    "plugins": [
      "transform-es2015-modules-commonjs"
    ]
  }
}
